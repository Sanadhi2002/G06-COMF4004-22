<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Project_G06</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/_Layout-cshtml.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Project_G06.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Project_G06</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
           <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="compare">Compare</a>
                        </li>
         
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="UnivProfile">University Profile</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="aboutUs">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="UnivDashController1" asp-action="UnivDashBoard">Uni dash</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="unireg">Registration for universities</a>
                        </li>
                   
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="Mynavbar">
        <a asp-area="" asp-controller="Home" asp-action="Index">Home</a>
        <a asp-area="" asp-controller="Home" asp-action="compare">Compare</a>
        
        <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        <a asp-area="" asp-controller="Home" asp-action="UnivProfile">University Profile</a>
        <a asp-area="" asp-controller="Home" asp-action="aboutUs">About Us</a>
        <a asp-area="" asp-controller="Home" asp-action="unireg">Registration for universities</a>
        <a asp-area="" asp-controller="Students" asp-action="Add">Register </a>
        <div class="Mynavbar_dropdown">
            <button class="Mynavbar_dropbtn">
                <a href="#">Register</a>
                <!--<i class="fa fa-caret-down"></i>-->
            </button>
            <div class="Mynavbar_dropdown-content">
                
                <a asp-area="" asp-controller="Home" asp-action="Register">Register for students</a>
          
               
            </div>
        </div>
        <div class="Mynavbar_dropdown">
            <button class="Mynavbar_dropbtn">
                <a href="#">Login</a>
                <!--<i class="fa fa-caret-down"></i>-->
            </button>
            <div class="Mynavbar_dropdown-content">
           
                <a asp-area="" asp-controller="Home" asp-action="LOGINSUPERADMIN">LOGIN for super admin</a>
            </div>
        </div>
       
    </div>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Project_G06 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
