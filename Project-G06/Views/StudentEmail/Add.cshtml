@{
}
@model Project_G06.Models.StudentEmail

<body class="ab" >
    <div class="login-boxs">
        <form method="post" action="Add" enctype="multipart/form-data" onsubmit="return ValidateForm()" >
 
              <br />

            <div class="user-boxs">
            <label for="S_Name">Name:</label>
            <br />
            <input type="text"  asp-for="S_Name" placeholder="Enter your name" id="name"/>
            <span asp-validation-for="S_Name" class="text-danger"></span>
          <!--<div>@Html.ValidationMessageFor(m => m.S_Name)</div>-->
            </div>

            <div class="user-boxs">
            <label for="S_Email">Email:</label>
            <br />
            <input type="email"  asp-for="S_Email" placeholder="Email Address" id="email"/>
            <span asp-validation-for="S_Email" class="text-danger"></span>
                <!--  <div>@Html.ValidationMessageFor(m =>m.S_Email)</div>-->
            </div>
      <br />
   
                
      <button type="submit" class="buttons" id="SignBtn">Sign Up</button>
   
     
    </form>


</div>
<br />
<br />
   

</body>

@section Scripts{

    <partial name="_ValidationScriptsPartial.cshtml" />
        }  }