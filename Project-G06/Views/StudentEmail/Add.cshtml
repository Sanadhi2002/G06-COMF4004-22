@model Project_G06.Models.StudentEmail
<h1>Register your email here !!!!!!!!!!!!</h1>
<form method="post" action="Add">

    <div class="mb-3">
        <label for="S_Name">Name:</label>
        <input type="text" class="form-control" asp-for="S_Name" />
    </div>

    <div class="mb-3">
        <label for="S_Email">Email:</label>
        <input type="email" class="form-control" asp-for="S_Email" />
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>

</form>