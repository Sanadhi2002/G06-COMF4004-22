@model List<Project_G06.Models.Universities>
@{
}
<h1>University list</h1>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Password</th>
            <th>Date</th>
            <th> </th>
   
           
        </tr>
    </thead>
    <tbody>
        @foreach (var universities in Model)
        {
            <tr>
                <td>@universities.Name</td>
                <td>@universities.Email</td>
                <td>@universities.Password</td>
                <td>@universities.Date.ToString("dd/mm/yy")</td>
               
                <td><button asp-action="View" asp-controller="Universities" type="submit">View</button></td>
                
     
            </tr>
        }

    </tbody>
</table>