@model IEnumerable<StudentEmail> //category is the name of the model



<p>
    @using (Html.BeginForm("Index", "Compare", FormMethod.Get))
    {

        @Html.RadioButton("SearchBy" , "S_Name" , true) <text>Name</text>

        @Html.RadioButton("SearchBy" , "S_Email") <text>Email </text>
            <br />
        @Html.TextBox("search")

            <input type="submit" value="Search" class="searchButton" id="searchButton" />
    }

</p>

