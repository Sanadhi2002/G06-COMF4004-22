@model IEnumerable<StudentEmail> //category is the name of the model


<h1>Individual uiversity profile</h1>



<div>
    <div>
        <a asp-controller="New" asp-action="Create" class="btn btn-primary">Register a new University</a>
    </div>
</div>



<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>University ID</th>
            <th>University Name</th>
            <th>University Email</th>
            <th></th>
         
        </tr>
    </thead>
   <tbody>
        
    @foreach (var obj  in Model                        /*model at the top of the page /  /*include the get action*/)
        {
            <tr>
                <td width="50%">
                    @obj.S_Id 
                </td>
                <td width="50%">
                    @obj.S_Name
                </td>
                <td width="50%">
                    @obj.S_Email
                </td>
               
                <td>
                    <div>
                        <a asp-controller="New" asp-action="Edit" asp-route-id="@obj.S_Id"> Edit</a>
                    </div>

                    <div>
                        <a asp-controller="New" asp-action="Delete" asp-route-id="@obj.S_Id"> Delete</a>

                    </div>

                    <div>
                        <a asp-controller="Profile" asp-action="Index" asp-route-id="@obj.S_Id"> Delete</a>

                    </div>

                </td>
            </tr>
        }
      





    </tbody>







</table>