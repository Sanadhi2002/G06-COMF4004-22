@model IEnumerable<UniProfileModel> //<>Holds the name of the model

<h1>Individual university profile</h1>


<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>University ID</th>
            <th>University Name</th>
            <th>University Short name</th>
            <th>Uni details</th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model /*model at the top of the page /  /*include the get action*/)
        {
            <tr>
                <td width="25%">
                    @obj.Uni_ID
                </td>
                <td width="25%">
                    @obj.Uni_FullName
                </td>
                <td width="25%">
                    @obj.Uni_ShortName
                </td>
                <td width="25%">
                    @obj.Uni_Details
                </td>


                <td>
                    <div>
                        <a asp-controller="New" asp-action="Edit" asp-route-id="@obj.Uni_ID"> Edit</a>
                    </div>

                    <div>
                        <a asp-controller="New" asp-action="Delete" asp-route-id="@obj.Uni_ID"> Delete</a>

                    </div>

                </td>
            </tr>
        }

    </tbody>







</table>